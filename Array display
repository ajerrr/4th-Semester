#include <stdio.h>

int main() {
    int numbers[10];
    
    // Scan 10 numbers into the array
    printf("Enter 10 numbers:\n");
    for(int i = 0; i < 10; i++) {
        printf("Number %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }

    // Print the numbers
    printf("The numbers you entered are:\n");
    for(int i = 0; i < 10; i++) {
        printf("%d ", numbers[i]);
    }

    return 0;
}
